<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Divider">
        <attr name="backgroundColor" format="color" />
        <attr name="lineSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DirTreeItemView">
        <attr name="title" format="string" />
        <attr name="titleColor" format="color" />
        <attr name="level" format="integer" />
        <attr name="isExpanded" format="boolean" />
        <attr name="type" format="string" />

    </declare-styleable>

    <declare-styleable name="WindowTab">
        <attr name="android:text" />
        <attr name="android:fontFamily" />
        <attr name="android:textStyle" />
    </declare-styleable>

    <declare-styleable name="InfoBar">
        <attr name="android:title" />
        <attr name="android:subtitle" />
    </declare-styleable>

    <declare-styleable name="StatusTag">
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:fontFamily" />
        <attr name="android:textSize" />
        <attr name="android:textStyle" />
        <attr name="android:drawable" />
        <attr name="borderWidth" format="dimension" />
        <attr name="borderColor" format="color" />
        <attr name="android:background" />
    </declare-styleable>

    <declare-styleable name="Badge">
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:fontFamily" />
        <attr name="android:textSize" />
        <attr name="android:textStyle" />
    </declare-styleable>

</resources>